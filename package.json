  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "@types/node": "^12.0.2",
    "jasmine": "^3.4.0",
    "ts-node": "^8.1.0"
  },
  "dependencies": {
    "typescript": "^3.4.5",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  }
