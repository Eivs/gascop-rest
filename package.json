{
  "name": "gascop-web",
  "version": "1.0.0",
  "description": "A REST interface for the gascop POCSAG paging system",
  "main": "index.js",
  "scripts": {
    "build": "mkdir -p build && rm -rf build/* && cp -r src_js/* build && npm run tsc",
    "buildWindowsPS": "rd -r build; mkdir build; cp src_js/* build; npm run tsc",
    "test": "ts-node node_modules/jasmine/bin/jasmine",
    "tsc": "tsc",
    "start": "PORT=1234 node ./build/bin/www"
  },
